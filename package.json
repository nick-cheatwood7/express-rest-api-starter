{
  "name": "express-rest-api-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "yarn docker:up && nodemon dist/src/index.js",
    "start": "node dist/src/index.js",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "repository": "https://github.com/nick-cheatwood7/express-rest-api-starter.git",
  "author": "nick-cheatwood7 <nick.cheatwood@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "argon2": "^0.28.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.4",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pino": "^8.3.0",
    "pino-pretty": "^8.1.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gen-env-types": "^1.3.4",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
